    "googleapis": "^19.0.0",
    "ramda": "^0.23.0",
    "request": "^2.81.0",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "babel-watch": "^2.0.6",
    "jasmine": "^2.5.3",
